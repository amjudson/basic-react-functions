    "babel-plugin-react-transform": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.3",
    "clean-webpack-plugin": "^1.0.1",
    "color": "^3.1.2",
    "concurrently": "^4.1.2",
    "css-loader": "^2.1.1",
    "es6-promise": "^4.2.8",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^3.0.1",
    "formik": "^1.5.8",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
