{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react"
  ],
  "plugins": [
    "add-module-exports",
    "@babel/transform-react-jsx",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
